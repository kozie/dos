SUB ZetLingoLetters(FileName$,X,Y)
  SHARED BarLen
  GOSUB WachtBar
  OPEN FileName$ FOR INPUT AS #1
    LINE INPUT #1, Formaat$
    Breedte=VAL(LEFT$(Formaat$,2))
    Hoogte=VAL(RIGHT$(Formaat$,2))
    SX%=X-Breedte/2
    SY%=Y-Hoogte/2
    WHILE NOT EOF(1)
      LINE INPUT #1, Regel$
      FOR N%=0 TO Breedte-1
        PSET(SX%+N%,SY%),VAL("&H"+MID$(Regel$,n%+1,1))
      NEXT N%
      SY%=SY%+1
    WEND
  CLOSE #1
END SUB

SUB TekstKader(Tekst$)
  SHARED GColor$,GLocate$,AG%,SchoonX,SchoonY,Schoon%()
  AG%=0
  BreedteFactor=8*INT(LEN(Tekst$)/2)
  SchoonX=290-BreedteFactor
  SchoonY=82
  GET(SchoonX,82)-(357+BreedteFactor,133),Schoon%
  LINE(290-BreedteFactor,82)-(349+BreedteFactor,127),7,BF
  LINE(298-BreedteFactor,128)-(357+BreedteFactor,133),0,BF
  LINE-(350+BreedteFactor,88),0,BF
  FOR N%=0 TO 1
    LINE(290+N%-BreedteFactor,127-N%)-(349-N%+BreedteFactor,127-N%),8
    LINE-STEP(0,-45+N%*2),8
    LINE-(290+N%-BreedteFactor,82+n%),15
    LINE-STEP(0,45-N%*2),15
    LINE(305-N%-BreedteFactor,94-N%)-(335-N%+Breedtefactor,115-N%),15-N%*7,B
  NEXT N%
  GCOLOR$="0,7"
  GLOCATE$="8,"+MID$(STR$(41-BreedteFactor/8),2,2)
END SUB

SUB LeesWoord                                 ' Leest een woord uit bestand
  SHARED WoordNr%,AantalWrd,TotaalWrdGew$,WrdGew$,NietGeweest%
  IF INSTR(TotaalWrdGew$,"-")=0 OR LEN(TotaalWrdGew$)-6 <> AantalWrd THEN
    WrdGew$=STRING$(AantalWrd,"-")
    NietGeweest%=AantalWrd
    TotaalWrdGew$=WrdGew$+LEFT$(STR$(NietGeweest%)+".000",6)
  ELSE
    WrdGew$=LEFT$(TotaalWrdGew$,AantalWrd)
    NietGeweest%=VAL(RIGHT$(TotaalWrdGew$,6))
  END IF
  WoordNr%=0
  Stap%=INT(RND*VAL(RIGHT$(TotaalWrdGew$,6)))+1
  FOR N%=1 to Stap%
    INCR WoordNr%
    WHILE MID$(WrdGew$,WoordNr%,1)="*"
      INCR WoordNr%
    WEND
  NEXT N%
  DECR NietGeweest%
  MID$(WrdGew$,WoordNr%,1)="*"
  TotaalWrdGew$=WrdGew$+LEFT$(STR$(NietGeweest%)+".000",6)
END SUB

SUB TekstInvoer(Wie)
  SHARED Woord$,Tekst$,Vraagteken,AantalTeGaan(),NogTeGaan(),TijdOm
  COLOR 7
  IF Vraagteken THEN
    COLOR 14
    LOCATE 8,57
    PRINT "  ";
    LINE(447,112)-STEP(17,0),15
    LINE-STEP(0,-14),15
    LINE-STEP(-17,0),8
    LINE-STEP(0,14),8
  ELSE
    LINE(88,322)-STEP(103,13),0,BF
    LINE(448,322)-STEP(103,13),0,BF
    LOCATE 24,13+Wie*45
    PRINT "Woord ";
    COLOR 15
    PRINT ".....";
  END IF
  XTekst$=""
  Tekst$=""
  I$=""
  IF VraagTeken=0 THEN
    ON TIMER(20) GOSUB TijdOm
    TIMER ON
  END IF
  TijdOm=0
  DO
    WHILE I$="" AND TijdOm=0
      I$=INKEY$
    WEND
    SELECT CASE I$
    CASE "a" TO "z" , "A" TO "Z"
      IF LEN(Tekst$)<5 AND Vraagteken=0 THEN
        Tekst$=Tekst$+UCASE$(I$)
      END IF
    CASE "0" TO "9"
      IF LEN(Tekst$)<2 AND Vraagteken=1 THEN
        Tekst$=Tekst$+I$
      END IF
    CASE CHR$(8) , CHR$(0)+CHR$(75)
      IF LEN(Tekst$)>0 THEN
        Tekst$=LEFT$(Tekst$,LEN(Tekst$)-1)
      END IF
    CASE CHR$(13)
      IF LEN(Tekst$)<5-4*Vraagteken THEN
        BEEP
      ELSE
        XTekst$=Tekst$
        TIMER OFF
        IF Vraagteken=1 THEN
          Tmp=0
          FOR N%=0 TO AantalTeGaan(Wie)
            IF NogTeGaan(Wie,N%)=VAL(Tekst$) THEN
              Tmp=1
            END IF
          NEXT N%
          IF Tmp THEN
            EXIT LOOP
          ELSE
            BEEP
          END IF
        ELSE
          EXIT LOOP
        END IF
      END IF
    END SELECT
    IF Vraagteken THEN
      LOCATE 8,57
      PRINT LEFT$(Tekst$+"  ",2);
    ELSE
      LOCATE 24,19+Wie*45
      PRINT LEFT$(Tekst$+".....",5);
    END IF
    I$=""
    IF TijdOm=1 THEN
      Tekst$=""
      EXIT LOOP
    END IF
  LOOP
  TIMER OFF
  IF LEFT$(Tekst$,1)=LEFT$(Woord$,1) OR LEN(Tekst$)=0 THEN
    LINE(88,322)-STEP(103,13),0,BF
    LINE(448,322)-STEP(103,13),0,BF
  END IF
END SUB

SUB Controle(Ingevoerd$)
  SHARED Woord$,AlGoed$,GoedFout$
  GoedFout$="00000"
  Help$=Woord$
  FOR N%=1 TO 5
    FOR M%=1 TO 5
      IF N%=M% AND MID$(Help$,N%,1)=MID$(Ingevoerd$,N%,1) THEN
        MID$(Algoed$,N%,1)=MID$(Help$,N%,1)
	MID$(Help$,N%,1)=" "
	MID$(Ingevoerd$,N%,1)="*"
	MID$(GoedFout$,N%,1)="1"
      END IF
    NEXT M%
  NEXT N%
  FOR N%=1 TO 5
    FOR M%=1 TO 5
      IF MID$(Help$,M%,1)=MID$(Ingevoerd$,N%,1) THEN
	MID$(Help$,M%,1)=" "
	MID$(Ingevoerd$,N%,1)="*"
	MID$(GoedFout$,N%,1)="2"
      END IF
    NEXT M%
  NEXT N%
END SUB

SUB Knipper
  PLAY "MB MS T110 O2 L8 G G MN P32 L16 G F F+ L8 G"
  FOR N=1 TO 6
    PALETTE 5,40
    PALETTE 6,24
    DELAY .04
    PALETTE 5,13
    PALETTE 6,35
    DELAY .04
  NEXT N
  FOR N=1 TO 4
    PALETTE 5,13
    PALETTE 6,24
    DELAY .2
    PALETTE 5,40
    PALETTE 6,35
    DELAY .2
  NEXT N
  PALETTE 5,13
END SUB

SUB PlaatsBal(X,Y,BalKleur)
  CIRCLE(X,Y),12,8
  PAINT(X,Y),8,8
  CIRCLE(X,Y),12,BalKleur-8
  PAINT(X,Y),BalKleur-8,BalKleur-8
  CIRCLE(X-1,Y-1),10,BalKleur
  PAINT(X,Y),BalKleur,BalKleur
  CIRCLE(X-4,Y-4),2,15
  PAINT(X-4,Y-4),15,15
END SUB


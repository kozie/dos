' ---------------------------------------------------------- Gosub-routines

ZetTekst:
  GColA=VAL(MID$(GColor$,INSTR(GColor$,",")+1,2))
  GColV=GColA XOR VAL(GColor$)
  GLocY=VAL(GLocate$)
  GLocX=VAL(MID$(GLocate$,INSTR(GLocate$,",")+1,2))
  IF AG%=0 THEN
    LINE(GLocX*8-8,GLocY*14-14)-(GLocX*8-1,GLocY*14-1),GColA,BF
  END IF
  GET(GLocX*8-8,GLocY*14-14)-(GLocX*8-1,GLocY*14-1),Gak%
  COLOR GColV
  FOR KarPlaats%=1 TO LEN(GPrint$)
    LOCATE GLocY,GLocX+KarPlaats%-1
    PRINT MID$(GPrint$,KarPlaats%,1);
    IF AG% THEN
      PUT((GLocX*8-8)+(KarPlaats%)*8-8,(GLocY*14-14)),Gak%,OR
    ELSE
      PUT((GLocX*8-8)+(KarPlaats%)*8-8,(GLocY*14-14)),Gak%,XOR
    END IF
  NEXT KarPlaats%
RETURN

WachtBar:
  SCREEN 9,,1,1
  BarLen=BarLen+1
  LINE(29+Barlen*5,325)-STEP(5,7),2,BF
  SCREEN 9,,0,1
RETURN

PlaatstWoord:
  DTijd=0
  FOR N%=1 TO 5
    IF GoedeTonen THEN
      IF Elaps!<270000 THEN
        DTijd=(300000-Elaps)/1000000
      ELSE
        DTijd=.03
      END IF
      DELAY DTijd
    END IF
    GF=VAL(MID$(GoedFout$,N%,1))
    IF MID$(TmpWoord$,N%,1)=>"A" AND MID$(TmpWoord$,N%,1)=<"Z" THEN
      MTIMER
      FOR Z%=0 TO 200
	Kar1%(Z%)=KarReeks%(GF,ASC(MID$(TmpWoord$,N%,1))-64,Z%)
      NEXT Z%
      IF GF=2 THEN
	CIRCLE(200+N%*40,165+Regel*30),15,14
	PAINT(200+N%*40,165+Regel*30),14,14
      END IF
      IF GF=1 THEN
	LINE(182+N%*40,152+Regel*30)-STEP(36,26),12,BF
      END IF
      IF TmpWoord$=Tekst$ THEN
        LINE(182+N%*40,152+Regel*30)-STEP(36,26),1,BF
      END IF
      PUT(192+N%*40,158+30*Regel),Kar1%,PSET
      Elaps!=MTIMER
      IF GoedeTonen THEN
        SOUND 200+SGN(GF)*600-GF*150,.5
      END IF
    ELSE
      LINE(199+N%*40,171+30*Regel)-STEP(3,1),15,B
    END IF
  NEXT N%
RETURN

Scroll:
  DELAY 1
  FOR N%=1 TO 4
    GET(220,180+N%*30)-(420,210+N%*30),Scroll%
    PUT(220,150+N%*30),Scroll%,PSET
    FOR M%=1 TO 5
      LINE(181+M%*40,181+N%*30)-STEP(38,28),1,BF
    NEXT M%
  NEXT N%
RETURN

BallenBak:
  IF Fase=1 THEN NogTrekken=2
HerHaal:
  GET(244,176)-(396,232),BallenBakWeg%
  CIRCLE(320,176),75,11,-PI,-.0000001
  PAINT(320,177),7,11
  CIRCLE(320,176),75,15,-PI,-.0000001
  CIRCLE(320,176),65,15,PI,3.6
  CIRCLE(320,176),64,15,PI,3.5
  CIRCLE(320,176),63,15,PI,3.4
BallenRollen:
  FOR N=1 TO 50
    Straal=61
    Hoek=RND*PI/2+PI*.25
    IF N>20 THEN
      DELAY .05
      IF N<40 THEN
	Straal=INT(RND*12)+50
      END IF
    END IF
    X=320+Straal*COS(Hoek)
    Y=176+Straal*ASP*SIN(Hoek)
    Bal=INT(RND*AantalTeGaan(Wie)+1)
    BalKleur=NogTeGaan(Wie,Bal)
    IF BalKleur<100 OR BalKleur=400 THEN BalKleur=9
    IF BalKleur=200 THEN BalKleur=10
    IF BalKleur=300 THEN BalKleur=12
    IF N<35 THEN BalKleur=9
    CALL PlaatsBal(X,Y,BalKleur)
  NEXT N
  IF Fase=2 AND NogTeGaan(Wie,Bal)=400 THEN
    BalKleur=14
  END IF
  CIRCLE(320,190),16,BalKleur-8
  PAINT(320,190),BalKleur-8,BalKleur-8
  CIRCLE(318,188),13,BalKleur
  PAINT(320,190),BalKleur,BalKleur
  CIRCLE(314,184),3,15
  PAINT(314,184),15,15
  IF BalKleur=10 THEN                     ' Groene bal getrokken
    DELAY 1
    Groen(Wie)=Groen(Wie)+1
    X=88+360*Wie+26*Groen(Wie)
    Y=235
    FOR MB=16 TO 0 STEP -.1
      CIRCLE(320,190),MB,6
      IF MB=<12 THEN CIRCLE(X,Y),12-MB,6
      SOUND 100,.1
    NEXT MB
    CIRCLE(320,190),16,8
    PAINT(320,190),7,8
    CIRCLE(320,190),16,7
    CALL PlaatsBal(X,Y,BalKleur)
    DELAY 1                              ' Na 3 groene terug in bak
    IF Groen(Wie)=3 THEN
      Gprint$="Jackpot voor TEAM"+STR$(Wie+1)
      CALL TekstKader(Gprint$)
      GOSUB ZetTekst
      DELAY 2
      PUT(SchoonX,SchoonY),Schoon%,PSET
      Score(2,Wie+1)=Score(2,Wie+1)+JackPot
      JackPot=0
      COLOR 12
      LOCATE 7,3
      PRINT "       0"
      LOCATE 3+Wie,19-LEN(STR$(Score(2,Wie+1)))
      PRINT Score(2,Wie+1)
    END IF
  END IF
  BeurtOver=0
  IF BalKleur=12 THEN                     ' Rode bal getrokken
    FOR N%=500 TO 100 STEP -10
      SOUND N%,.5
      SOUND N%-50,.5
    NEXT N%
    BeurtOver=1
    NogTrekken=0
  END IF
  IF BalKleur=9 THEN                      ' Blauwe bal, cijfer tonen
    CIJFER=NogTeGaan(Wie,Bal)
    FOR N%=1 TO 7
      IF NogTeGaan(Wie,Bal)=400 THEN
        Tmp$=BalCijfer$(10,N%)
      ELSE
        IF Cijfer>9 THEN
          TMP$=BalCijfer$(INT(Cijfer/10),N%)+"11"+BalCijfer$(Cijfer MOD 10,N%)
        ELSE
          Tmp$=BalCijfer$(Cijfer,N%)
        END IF
      END IF
      FOR M%=1 TO LEN(Tmp$)
        IF MID$(Tmp$,M%,1)="f" THEN
          PSET(319-INT(LEN(Tmp$)/2)+M%,188+N%),14
        END IF
      NEXT M%
    NEXT N%
  END IF
  IF BalKleur<>14 THEN
    AantalTeGaan(Wie)=AantalTeGaan(Wie)-1
    FOR N%=Bal to AantalTeGaan(Wie)
      SWAP NogTeGaan(Wie,N%),NogTeGaan(Wie,N%+1)
    NEXT N%
  END IF
  IF Groen(Wie)=3 THEN
    Groen(Wie)=0
    FOR N%=AantalTeGaan(Wie)+1 to AantalTeGaan(Wie)+3
      NogTeGaan(Wie,N%)=200
    NEXT N%
    AantalTeGaan(Wie)=AantalTeGaan(Wie)+3
    IF Wie=0 THEN
      PUT(100,226),GroenPlaats1%,PSET
    ELSE
      PUT(460,226),GroenPlaats2%,PSET
    END IF
  END IF
  IF BalKleur=10 THEN GOTO BallenRollen
  DELAY 2
  PUT(244,176),BallenBakWeg%,PSET
  IF NogTeGaan(Wie,AantalTeGaan(Wie)+1)=400 AND Fase=1 THEN
      Gprint$="Een vraagteken... Welke weghalen?   "
      CALL TekstKader(Gprint$)
      GOSUB ZetTekst
      Vraagteken=1
      CALL TekstInvoer(Wie)
      Cijfer=VAL(Tekst$)
      PUT(SchoonX,SchoonY),Schoon%,PSET
  END IF
  IF BalKleur=14 THEN
      Gprint$="Lucky you...  U heeft de goude bal"
      CALL TekstKader(Gprint$)
      GOSUB ZetTekst
      NogTrekken=0
      DELAY 2
      PUT(SchoonX,SchoonY),Schoon%,PSET
  END IF
  IF BalKleur=9 THEN GOSUB NummerWeghalen
  Vraagteken=0
  NogTrekken=NogTrekken-1
  IF NogTrekken>0 THEN GOTO Herhaal
  IF BeurtOver=1 THEN Wie=ABS(Wie-1)
RETURN

KaartenDelen:
  FOR T%=0 TO 2-Fase
    IF Fase=2 THEN T%=Wie
    Gprint$="Nieuwe kaart voor TEAM"+STR$(T%+1)
    CALL TekstKader(Gprint$)
    GOSUB ZetTekst
    DELAY 1
    FOR N%=1 TO 5
      FOR M%=1 TO 5
        SCREEN 9,,1,0
        LINE(1,1)-(40,40),1,BF
        Gprint$=STR$(Kaart(T%,M%,N%))
        Gcolor$="15,1"
        Glocate$="2,2"
        GOSUB ZetTekst
        IF Kaart(T%,M%,N%)<10 THEN
          GET(11,12)-(27,28),Kar1%
        ELSE
          GET(16,12)-(32,28),Kar1%
        END IF
        SCREEN 9,,0,0
        SOUND 120,.7
        PUT(192+M%*40,157+N%*30),Kar1%,PSET
	SOUND 30000,.7
      NEXT M%
    NEXT N%
    DELAY .5
    FOR N%=1 TO 5
      FOR M%=1 TO 5
        ReedsWeg(1,M%,N%)=ReedsWeg(0,M%,N%)
        ReedsWeg(2,M%,N%)=ReedsWeg(0,M%,N%)
        ReedsWeg(3,M%,N%)=1
        IF M%=3 AND N%=3 THEN ReedsWeg(3,3,3)=0
        IF (M% MOD 2)=0 AND (N%=1 OR N%=5) THEN ReedsWeg(3,M%,N%)=0
        IF (N% MOD 2)=0 AND (M%=1 OR M%=5) THEN ReedsWeg(3,M%,N%)=0
	IF ReedsWeg(Fase+1,M%,N%) THEN
          SOUND 300,.7
          SOUND 30000,.5
	  CIRCLE(200+M%*40,165+N%*30),15,14
	  PAINT (200+M%*40,165+N%*30),14,14
          SOUND 800,.7
          DELAY .23
	END IF
      NEXT M%
    NEXT N%
    PUT(SchoonX,SchoonY),Schoon%,PSET
    DELAY 1
    CALL Schoon
  NEXT T%
  ERASE NogTeGaan
  FOR Z%=0 TO 1
    Tel=0
    FOR N%=1 TO 5
      FOR M%=1 TO 5
        IF ReedsWeg(0,M%,N%)=0 OR Fase=2 THEN
          NogTeGaan(Z%,Tel)=Kaart(Z%,M%,N%)
          Tel=Tel+1
        END IF
      NEXT M%
    NEXT N%
    Tel=Tel-1
    IF Fase=1 THEN
      NogTeGaan(Z%,17)=400
      FOR Tel=18 TO 23
        IF Tel<21 THEN
          NogTeGaan(Z%,Tel)=300
        ELSE
          NogTeGaan(Z%,Tel)=200
        END IF
      NEXT Tel
      Tel=23-Groen(Z%)
    END IF
    AantalTeGaan(Z%)=Tel
    IF Fase=2 THEN
      NogTeGaan(Z%,25)=400
      AantalTeGaan(Z%)=25
    END IF
  NEXT Z%
  DELAY 1
RETURN

KaartTonen:
  DELAY 1
  IF Fase=2 THEN
    ReedsWeg=3
  ELSE
    ReedsWeg=Wie+1
  END IF
  FOR N%=1 TO 5
    FOR M%=1 TO 5
      LINE(181+M%*40,151+N%*30)-STEP(38,28),1,BF
      IF ReedsWeg(ReedsWeg,M%,N%)=0 THEN
	SCREEN 9,,1,0
	LINE(1,1)-(40,40),1,BF
	Gprint$=STR$(Kaart(Wie,M%,N%))
	Gcolor$="15,1"
	Glocate$="2,2"
	GOSUB ZetTekst
	IF Kaart(Wie,M%,N%)<10 THEN
	  GET(11,12)-(27,28),Kar1%
	ELSE
	  GET(16,12)-(32,28),Kar1%
	END IF
	SCREEN 9,,0,0
        PUT(192+M%*40,157+N%*30),Kar1%,PSET
      ELSE
	CIRCLE(200+M%*40,165+N%*30),15,14
	PAINT (200+M%*40,165+N%*30),14,14
      END IF
    NEXT M%
  NEXT N%
  DELAY 2
RETURN

NummerWeghalen:
  FOR N%=1 TO 5
    FOR M%=1 TO 5
      IF Kaart(Wie,M%,N%)=Cijfer THEN
	IF Fase=2 AND ReedsWeg(ReedsWeg,M%,N%)=1 THEN
	  FOR LX%=1 TO 5
	    FOR LY%=1 TO 5
	      IF ReedsWeg(ReedsWeg,LX%,LY%)=1 THEN
		FOR LN%=-5 TO 5 STEP 10
		  CIRCLE(200+LX%*40+LN%,160+LY%*30),2,4
		  PAINT (200+LX%*40+LN%,160+LY%*30),4,4
		NEXT LN%
		CIRCLE(200+LX%*40,161+LY%*30),14,4,3.9,5.5
		LINE(190+LX%*40,168+LY%*30)-STEP(20,0),4
		PAINT(200+LX%*40,170+LY%*30),4,4
		SOUND 500+LX%*60+LY%*12,1
                SOUND 30000,.5
	      END IF
	    NEXT LY%
	  NEXT LX%
	END IF

        IF Fase=1 OR ReedsWeg(ReedsWeg,M%,N%)=0 THEN
          IF ReedsWeg(ReedsWeg,M%,N%)=1 THEN
            Gprint$="Ja... Die is met het vraagteken al weggehaald"
            CALL TekstKader(Gprint$)
            GOSUB ZetTekst
          END IF
          FOR L%=1 TO 9
	    SOUND 300+500*(L% MOD 2),1
	    CIRCLE(200+M%*40,165+N%*30),15,1+13*(L% MOD 2)
	    DELAY .2
	  NEXT L%
	  PAINT(200+M%*40,165+N%*30),14,14
          IF ReedsWeg(ReedsWeg,M%,N%)=1 THEN DELAY 2
          PUT (SchoonX,SchoonY),Schoon%,PSET
        END IF

	IF Fase=2 AND ReedsWeg(ReedsWeg,M%,N%)=0 THEN
	  FOR LN%=-5 TO 5 STEP 10
	    CIRCLE(200+M%*40+LN%,160+N%*30),2,4
	    PAINT (200+M%*40+LN%,160+N%*30),4,4
	  NEXT LN%
	  LINE(192+M%*40,160+N%*30)-STEP(4,-2),4
	  LINE(208+M%*40,160+N%*30)-STEP(-4,-2),4
	  CIRCLE(200+M%*40,175+N%*30),10,4,.6,2.6
	  LINE(192+M%*40,171+N%*30)-STEP(16,0),4
	  PAINT(200+M%*40,170+N%*30),4,4
	  FOR HB%=500 TO 40 STEP -20
	    SOUND HB%+40,.75
            SOUND HB%,.75
	  NEXT HB%
	END IF

	LingoX%=M%:LingoY%=N%
	ReedsWeg(ReedsWeg,M%,N%)=1
        HorLingo=0
        VerLingo=0
        Diag1Lingo=0
        Diag2Lingo=0
        FOR Z%=1 TO 5
          IF ReedsWeg(ReedsWeg,Z%,N%)=1 THEN HorLingo=HorLingo+1
          IF ReedsWeg(ReedsWeg,M%,Z%)=1 THEN VerLingo=VerLingo+1
          IF ReedsWeg(ReedsWeg,Z%,Z%)=1 THEN Diag1Lingo=Diag1Lingo+1
          IF ReedsWeg(ReedsWeg,Z%,6-Z%)=1 THEN Diag2Lingo=Diag2Lingo+1
        NEXT Z%
	IF HorLingo=5 OR VerLingo=5 OR Diag1Lingo=5 OR Diag2Lingo=5 THEN
	  DO
	    IF HorLingo=5 THEN
	      FOR LingoX%=1 TO 5
                LingoDeel=LingoX%
		GOSUB ShowLingo
	      NEXT LingoX%
	      EXIT LOOP
	    END IF
	    IF VerLingo=5 THEN
	      FOR LingoY%=1 to 5
                LingoDeel=LingoY%
		GOSUB ShowLingo
	      NEXT LingoY%
	      EXIT LOOP
	    END IF
	    IF Diag1Lingo=5 THEN
	      FOR LingoX%=1 TO 5
		LingoDeel=LingoX%
		LingoY%=LingoX%
		GOSUB ShowLingo
	      NEXT LingoX%
	      EXIT LOOP
	    END IF
	    IF Diag2Lingo=5 THEN
	      FOR LingoX%=1 to 5
		LingoDeel=LingoX%
		LingoY%=6-LingoX%
		GOSUB ShowLingo
	      NEXT LingoX%
	      EXIT LOOP
	    END IF
	  LOOP
          Lingo=1
          NogTrekken=0
          IF Fase=1 THEN
	    CALL Knipper
	    Score(1,Wie+1)=Score(1,Wie+1)+100
	    Score$=MID$(STR$(Score(1,Wie+1)),2,5)
	    COLOR 12
	    LOCATE 20,21-LEN(Score$)+Wie*45
	    PRINT Score$
	  ELSE
	    FOR S1=1 to 4
	      FOR S2=1 to 75
		SOUND 500-55*S1-S2,.05
	      NEXT S2
	      DELAY .2
	    NEXT S1
	    FOR S1=225 to 50 STEP -1
	      SOUND S1,.1
	    NEXT S1
          END IF
        END IF
      END IF
    NEXT M%
  NEXT N%
  DELAY 2
  IF Fase=2 AND Lingo=0 THEN
    FOR LX%=1 TO 5
      FOR LY%=1 TO 5
	IF ReedsWeg(ReedsWeg,LX%,LY%)=1 THEN
	  FOR LN%=-5 TO 5 STEP 10
	    PAINT (200+LX%*40+LN%,160+LY%*30),14,14
	  NEXT LN%
	  PAINT(200+LX%*40,170+LY%*30),14,14
	END IF
      NEXT LY%
    NEXT LX%
  END IF
RETURN

ShowLingo:
  Lingo$="LINGO"
  FOR Z%=0 TO 200
    Kar1%(Z%)=KarReeks%(1,ASC(MID$(Lingo$,LingoDeel,1))-64,Z%)
  NEXT Z%
  LINE(182+LingoX%*40,152+LingoY%*30)-STEP(36,26),12,BF
  PUT(192+LingoX%*40,158+30*LingoY%),Kar1%,PSET
RETURN

HetWoordWas:
  Gprint$="Tja... Dat is jammer, niet geraden! Het woord was....."
  CALL TekstKader(Gprint$)
  GOSUB ZetTekst
  DELAY 1
  GOSUB Scroll
  DELAY 1
  GoedeTonen=0
  GoedFout$="00000"
  TmpWoord$=AlGoed$
  GOSUB PlaatstWoord
  DELAY 1
  GoedeTonen=1
  GoedFout$="11111"
  TmpWoord$=Woord$
  GOSUB PlaatstWoord
  DELAY 2
  PUT(SchoonX,SchoonY),Schoon%,PSET
RETURN

DoorgaanVraag:
  FOR N%=1 TO 126
    FOR M%=1 TO 9
      IF RunEsc$(M%,N%)="1" THEN
	PSET(256+N%,115+M%),14
	PSET(257+N%,116+M%),0
      END IF
    NEXT M%
  NEXT N%
  Gcolor$="0"
  WHILE INKEY$<>""
  WEND
  WHILE NOT INSTAT
  WEND
  PUT(SchoonX,SchoonY),Schoon%,PSET
'  LINE(100+Wie*360,245)-STEP(80,70),13,B
  IF INKEY$=CHR$(27) THEN
    Gprint$="Wil je echt stoppen? [J/N]"
    CALL TekstKader(Gprint$)
    GOSUB ZetTekst
    WHILE INKEY$<>"":WEND
    I$=""
    WHILE I$=""
      I$=INKEY$
    WEND
    IF I$="j" OR I$="J" THEN GOTO Eind
    PUT(SchoonX,SchoonY),Schoon%,PSET
  END IF
  CALL LeesWoord
  GET #1, WoordNr%
  GespeeldeWoorden=GespeeldeWoorden+1
RETURN

TijdOm:
  IF LEN(XTekst$)<5 THEN
    TijdOm=1
    PUT(SchoonX,SchoonY),Schoon%,PSET
    Gprint$="Denktijd is om"
    CALL TekstKader(Gprint$)
    GOSUB ZetTekst
    SOUND 450,5
    SOUND 150,10
    DELAY 2
    PUT(SchoonX,SchoonY),Schoon%,PSET
  END IF
RETURN

GeleKader:
  FOR GK=14*Wie TO 14-14*Wie STEP -((2*Wie)-1)
    LINE(100,245)-(180,315),14*(1-Wie),B,2^GK
    LINE(460,245)-(540,315),14*Wie,B,2^GK
    DELAY .05
  NEXT GK
  LINE(100,245)-(180,315),14*(1-Wie),B
  LINE(460,245)-(540,315),14*Wie,B
RETURN

